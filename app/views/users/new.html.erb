<h1>Create an Account</h1>

<%= form_for(@user) do |f| %>
    <%= f.label(:first_name) %><br>
    <%= f.text_field(:first_name) %><br>

    <%= f.label(:last_name) %><br>
    <%= f.text_field(:last_name) %><br>

    <%= f.label(:email) %><br>
    <%= f.text_field(:email) %><br>

    <%= f.label(:password) %><br>
    <%= f.text_field(:password) %><br>

    <%= f.submit %>
<% end %>

<br><br><br><br>

<h3>Address Later in Development</h3>
<p>name:string</p>
<p>image:string</p>
<p>uid:string</p>